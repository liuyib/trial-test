{
  "name": "opensource-test",
  "version": "0.0.2",
  "description": "开源项目开发流程测试",
  "main": "src/index.js",
  "keywords": [
    "opensource"
  ],
  "author": {
    "name": "liuyibo",
    "url": "https://github.com/liuyib"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/liuyib/openSource-test.git"
  },
  "bugs": {
    "url": "https://github.com/liuyib/openSource-test/issues"
  },
  "scripts": {
    "dev": "webpack --mode development",
    "build": "webpack --mode production",
    "coverage": "./node_modules/.bin/istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- -R spec",
    "codecov": "cat ./coverage/lcov.info | ./node_modules/.bin/codecov | ./node_modules/coveralls/bin/coveralls",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs"
  },
  "homepage": "https://github.com/liuyib/openSource-test#readme",
  "babel": {
    "presets": [
      "es2015",
      [
        "env",
        {
          "target": {
            "browsers": [
              "last 1 version",
              "> 1%",
              "not dead",
              "ie >= 10"
            ]
          }
        }
      ]
    ],
    "env": {
      "test": {
        "plugins": [
          "istanbul"
        ]
      }
    },
    "plugins": []
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^4.2.0",
    "codecov": "^3.5.0",
    "coveralls": "^3.0.3",
    "istanbul": "^0.4.5",
    "mocha": "^6.1.2",
    "should": "^13.2.3",
    "vuepress": "^0.14.11",
    "webpack": "^4.28.4",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.3.1"
  },
  "dependencies": {}
}
